<aura:component implements="flexipage:availableForAllPageTypes" controller='CarController'>
    <aura:attribute name="cars" type="Object"/>
    <aura:attribute name="columnsCar" type="List"/>
    <aura:handler name="init" value="{! this }" action="{! c.init }"/>
    <lightning:datatable data="{! v.cars }"
        columns="{! v.columnsCar }"
        keyField="Id"
        hideCheckboxColumn="true"/>
</aura:component>	
